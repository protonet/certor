{
  "name": "clavator",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "start": "nodemon ./dist/server.js server",
    "pretest": "npm run build",
    "test": "mocha --debug --require ./dist/etcd_daemon.js dist/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/certor"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/certor/issues"
  },
  "homepage": "https://github.com/mabels/certor#readme",
  "devDependencies": {
    "awesome-typescript-loader": ">=2.2.4",
    "ts-loader": ">=1.3.3",
    "babel-core": ">=6.9.1",
    "babel-plugin-transform-async-to-plain-generator": "1.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": ">=6.10.3",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "@types/chai": ">=3.4.33",
    "chai": ">=3.5.0",
    "cz-conventional-changelog": ">=1.1.6",
    "@types/mocha": ">=2.2.33",
    "mocha": ">=3.2.0",
    "nodemon": ">=1.9.2",
    "rimraf": ">=2.5.2",
    "source-map-support": ">=0.4.1",
    "tslint": ">=3.11.0",
    "typescript": "=2.1.1",
    "webpack": ">=1.14.0",
    "globby": ">=5.0.0"
  },
  "dependencies": {
    "sshpk": ">=1.10.1",
    "ipaddress": ">=0.1.0",
    "es6-shim": ">=0.35.2",
    "@types/es6-shim": ">=0.31.32",
    "fs-promise": ">=1.0.0",
    "@types/fs-promise": ">=1.0.1",
    "babel-polyfill": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-es2016": "^6.1.4",
    "@types/node-uuid": ">=0.0.28",
    "node-uuid": ">=1.4.7",
    "promise-etcd": ">=1.0.2"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
